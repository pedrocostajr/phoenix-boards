{
    "version": 2,
    "framework": "vite",
    "rewrites": [
        {
            "source": "/debug_login.html",
            "destination": "/debug_login.html"
        },
        {
            "source": "/assets/(.*)",
            "destination": "/assets/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}